x1, y1 = map(int, input().split())
x2, y2 = map(int, input().split())
x3, y3 = map(int, input().split())
x4, y4 = map(int, input().split())
a = x1-y1
b = x2-y2
c = x3-y3
d = x4-y4
e = [a, b, c, d]
f = []
for i in e:
    if i >= 0:
        f.append(i)
    else:
        f.append(-i)
for i in f:
    if f.count(i) == 2:
        print('yes')
        break
    else:
        print('no')
        break
